package Controller;

import Bo.FuncionarioBO;

public class CadastroController {
	    private FuncionarioBO funcionarioBO;

	    public CadastroController(FuncionarioBO funcionarioBO) {
	        this.funcionarioBO = funcionarioBO;
	    }

	    public boolean cadastrarFuncionario(String nomeCompleto, String celular, String email, String senha, String dataNascimento, String genero) {
	        funcionarioBO.Cadastrar(nomeCompleto, celular, email, senha, dataNascimento, genero);
			return false;
	    }

	    // Você pode adicionar outros métodos aqui, se necessário, para coordenar outras ações relacionadas ao cadastro
	}



