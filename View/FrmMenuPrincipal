    private void btnconfiguracoesActionPerformed(java.awt.event.ActionEvent evt) {                                                 
      
        new FrmMenuConfiguracoes().setVisible(true);
        setVisible(false);
        
    }                                                

    private void Controle_estoqueActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        FrmControleEstoque tela = new FrmControleEstoque();
        tela.setVisible(true);
         this.dispose();
    }                                                

    private void historico_vendasActionPerformed(java.awt.event.ActionEvent evt) {                                                 
     
       new FrmHistoricoVendas().setVisible(true);
       this.dispose();
    }                                                

    private void jMenuItem7ActionPerformed(java.awt.event.ActionEvent evt) {                                           

       new FrmVendas().setVisible(true);
       this.dispose();
    }                                          

    private void jMenuItem13ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        
        new FrmSobreSistema().setVisible(true);
        this.dispose();
    }                                           

