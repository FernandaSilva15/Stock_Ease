    }                                        

    private void jMenu6MouseClicked(java.awt.event.MouseEvent evt) {                                    
        int op;
        op = JOptionPane.showConfirmDialog(null, "Deseja sair da Sistema?");
        if (op == 0) {
            System.exit(0);
        }
    }                                   

