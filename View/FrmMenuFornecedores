    private void btnconsultaforActionPerformed(java.awt.event.ActionEvent evt) {                                               

        FrmFornecedores tela = new FrmFornecedores();
        tela.jTabbedPane1.setSelectedIndex(1);
        tela.setVisible(true);
    }                                              

    private void formWindowActivated(java.awt.event.WindowEvent evt) {                                     
        //maximizar frame
        this.setExtendedState(this.MAXIMIZED_BOTH);
    }                                    

    private void btncadastroforActionPerformed(java.awt.event.ActionEvent evt) {                                               
       this.dispose();
        new FrmFornecedores().setVisible(true);
    }                                              

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        this.dispose();
        new FrmMenuPrincipal().setVisible(true);
       
    }                                        

